		"express": "^4.17.1",
		"http-errors": "^1.8.0",
		"lodash": "^4.17.19"
	},
	"devDependencies": {
		"@types/http-errors": "^1.8.0",
		"@types/node": "^12.12.50",
		"@typescript-eslint/parser": "^3.6.1",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"chai-connect-middleware": "^0.3.1",
		"cross-env": "^7.0.2",
		"faker": "^4.1.0",
		"mocha": "^8.0.1",
		"mocha-prepare": "^0.1.0",
		"nodemon": "^2.0.4",
		"path-to-regexp": "^6.1.0",
		"sinon": "^9.0.2",
		"sinon-chai": "^3.5.0",
